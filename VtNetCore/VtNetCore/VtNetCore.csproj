<Project Sdk="Microsoft.NET.Sdk" ToolsVersion="15.0">
  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Darren R. Starr</Authors>
    <Company>Telenor Inpli AS</Company>
    <Product>VtNet terminal emulation library</Product>
    <Version>0.0.2</Version>
    <Description>A VT100/XTerm style terminal emulator library for the .NET Standard 2.0 framework</Description>
    <Copyright>2018 Darren R. Starr - Telenor Inpli AS</Copyright>
    <PackageProjectUrl>https://github.com/darrenstarr/VtNetCore</PackageProjectUrl>
    <RepositoryUrl>https://github.com/darrenstarr/VtNetCore</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
    <PackageLicense>https://github.com/darrenstarr/VtNetCore/blob/master/LICENSE</PackageLicense>
    <PackageTags>VT-100 VT220 xterm NetCore</PackageTags>
    <PackageReadmeFile>Keyboard.md</PackageReadmeFile>
  </PropertyGroup>
  <ItemGroup>
	  <None Include="Keyboard.md" Pack="true" PackagePath="\" />
  </ItemGroup>
</Project>